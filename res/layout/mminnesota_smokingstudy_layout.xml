<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LL1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center_vertical|fill_vertical|center_horizontal|fill_horizontal"
    android:gravity="center_vertical|fill_vertical|center_horizontal|fill_horizontal"
    android:orientation="vertical" >

    <TableLayout
        android:id="@+id/TL11"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#ff000f16"
        android:orientation="vertical" >

        <TableRow
            android:id="@+id/TR111"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/TV_waketext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Wakeup Time:"
                android:textColor="#ff6699ff" >
            </TextView>

            <TextView
                android:id="@+id/TV_wakeuptime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="8:00 am"
                android:textStyle="bold" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TR112"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/TV_sleeptext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sleep Time:"
                android:textColor="#ff1d9624" >
            </TextView>

            <TextView
                android:id="@+id/TV_sleeptime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10:00 pm"
                android:textStyle="bold" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TR113"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/salivatext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Saliva Alarm Time:"
                android:textColor="#ffffff99" >
            </TextView>

            <TextView
                android:id="@+id/TV_saliva"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="time"
                android:textStyle="bold" >
            </TextView>
        </TableRow>
    </TableLayout>
 
    <LinearLayout
        android:id="@+id/LL1222"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="vertical" >
        
    </LinearLayout>
