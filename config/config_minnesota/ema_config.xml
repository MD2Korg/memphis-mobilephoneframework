<?xml version="1.0" encoding="utf-8"?>
<fieldstream>
	<emaconfig>
		<min_time_between_ema>18</min_time_between_ema>
		<max_number_emas>8</max_number_emas>
		<starttimeout>5</starttimeout>
		<interviewtimeout>7</interviewtimeout>
		<beeptime>2</beeptime>
		<userdelay>10</userdelay>
		<emafiles>
			<emafile><filename>default_questions.xml</filename></emafile>
			<emafile><filename>lapse_questions.xml</filename></emafile>
			<emafile><filename>saliva_questions.xml</filename></emafile>
			<emafile><filename>saliva_ema_questions.xml</filename></emafile>
		</emafiles>
		<defaultemafiles>default_questions.xml</defaultemafiles>
		<event_triggered_ema>			
			<event>
				<model>MODEL_SELF_CRAVING</model>
				<budget>2</budget>
				<expected_number>2</expected_number> 
				<delay>0</delay>
				<filename>default_questions.xml</filename>				
				<level>flexible</level>
			</event>
			<event>
				<model>MODEL_SELF_SMOKING</model>
				<budget>2</budget>
				<expected_number>2</expected_number>
				<delay>10</delay>
				<filename>lapse_questions.xml</filename>
				<level>flexible</level>
			</event>
			<event>
				<model>MODEL_SELF_CONVERSATION</model>
				<budget>2</budget>
				<expected_number>4</expected_number>
				<delay>0</delay>
				<filename>default_questions.xml</filename>				
				<level>flexible</level>								
			</event>
			<event>
				<model>MODEL_SELF_STRESS</model>
				<budget>2</budget>
				<expected_number>3</expected_number>
				<delay>0</delay>
				<filename>default_questions.xml</filename>
				<level>flexible</level>								
			</event>
			<event>
				<model>MODEL_COLLECT_SALIVA</model>
				<budget>8</budget>
				<expected_number>8</expected_number> 
				<delay>0</delay>
				<filename>saliva_questions.xml</filename>
				<level>rigid</level>
			</event>
			
		</event_triggered_ema>
	</emaconfig>
</fieldstream>
